for(long long i = 0; i<k;i++){
            vec[i]= a*multiplos[i]+b/multiplos[i];
            cout <<"esse";
        }
        for(long long i = k-1; i>=0;i--){
           if(vec[i]%2 == 0){
                cout << vec[i] << endl;
                break;
            }else if(i==0 && vec[i]%2 != 0){
              cout << -1 << endl;
            }
        }